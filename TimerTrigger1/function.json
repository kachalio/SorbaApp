{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 */1 * * *",
      "runOnStartup": false
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outputBlob",
      "path": "storage-dev/{DateTime}.json",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
